Algoritmo "semnome"

Var
   nome: caractere
   idade: inteiro
Inicio
        escreval ("Digite seu nome")
        leia (nome)
        escreval ("Informe sua idade")
        leia (idade)
        
        escreval("Dados do cliente ")
        escreval("Nome: ",nome," ", "Tem", idade," ", "de idade")


Fimalgoritmo