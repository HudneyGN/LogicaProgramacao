Algoritmo "consumo"

Var
   distancia, consumo, consumoMedia : real
Inicio
      escreva("Distancia total da viagem: ")
      leia(distancia)
      escreva("Consumo de combustivel: ")
      leia(consumo)
      
      consumoMedia <-  distancia / consumo
      escreval("Consumo medio: ", consumoMedia:5:3)
Fimalgoritmo